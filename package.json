{
  "name": "circos",
  "version": "2.2.0",
  "description": "A d3js library to build circos graphs",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "NODE_ENV=test mocha --require @babel/register  --require ignore-styles --reporter spec src/**/*.test.js src/*.test.js",
    "test:tdd": "NODE_ENV=test mocha --require @babel/register --require ignore-styles --reporter nyan --watch src/*.test.js src/**/*.test.js",
    "coveralls": "CIRCOSJS_COVERAGE=1 mocha test --reporter mocha-lcov-reporter src/**/*.test.js | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack --watch",
    "build": "NODE_ENV=prod webpack --config webpack.config.lib.js && webpack --config webpack.config.lib.js && webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicgirault/circosJS.git"
  },
  "author": "Nicolas Girault",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicgirault/circosJS/issues"
  },
  "homepage": "https://github.com/nicgirault/circosJS",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-modules-umd": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@types/lodash-es": "^4.17.12",
    "babel-loader": "^8.0.0",
    "chai": "5.1.1",
    "coveralls": "^3.0.1",
    "css-loader": "^7.1.2",
    "ignore-styles": "^5.0.1",
    "jsdom": "24.1.1",
    "jsdom-global": "^3.0.2",
    "mocha": "8.3.0",
    "mocha-jsdom": "^1.1.0",
    "mocha-lcov-reporter": "^1.2.0",
    "sinon": "18.0.0",
    "sinon-chai": "^4.0.0",
    "standard": "^17.1.0",
    "style-loader": "^0.21.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "clipboard-js": "^0.3.6",
    "d3": "^7.9.0",
    "d3-scale-chromatic": "^3.1.0",
    "lodash-es": "^4.17.10"
  }
}
