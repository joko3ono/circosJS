{
  "name": "circos",
  "version": "2.2.0",
  "description": "A d3js library to build circos graphs",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "test:tdd": "NODE_ENV=test jest -r @babel/register --require ignore-styles --reporter nyan --watch src/*.test.js src/**/*.test.js",
    "coveralls": "CIRCOSJS_COVERAGE=1 mocha test --reporter mocha-lcov-reporter src/**/*.test.js | ./node_modules/coveralls/bin/coveralls.js",
    "start": "webpack --watch",
    "build": "NODE_ENV=prod webpack --config webpack.config.lib.js && webpack --config webpack.config.lib.js && webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nicgirault/circosJS.git"
  },
  "author": "Nicolas Girault",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicgirault/circosJS/issues"
  },
  "homepage": "https://github.com/nicgirault/circosJS",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-modules-umd": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.25.0",
    "@babel/register": "^7.24.6",
    "babel-loader": "^8.0.0",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "ignore-styles": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "jsdom": "^24.1.1",
    "jsdom-global": "^3.0.2",
    "nodemon": "^3.1.4",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "util": "^0.12.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "clipboard-js": "^0.3.6",
    "d3": "^7.9.0",
    "d3-scale-chromatic": "^1.3.0",
    "lodash": "^4.17.10"
  }
}
